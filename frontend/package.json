{
  "name": "happy-tts-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "sideEffects": [
    "*.css",
    "*.scss",
    "*.sass",
    "*.less"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:simple": "tsc && vite build --mode simple",
    "build:minimal": "tsc && vite build --mode minimal",
    "build:analyze": "tsc && vite build --mode analyze",
    "build:optimized": "tsc && vite build --mode production",
    "preview": "vite preview",
    "serve": "node static-server.js",
    "serve:prod": "npm run build && node static-server.js",
    "test:rate-limit": "node test-rate-limit.js",
    "security-check": "node security-check.js",
    "test": "vitest",
    "analyze": "npm run build:analyze && npx vite-bundle-analyzer dist",
    "analyze:bundle": "node scripts/analyze-bundle.js",
    "fix:rollup": "node fix-rollup-deps.js",
    "fix:rollup:win": "node fix-rollup-win.js",
    "fix:rollup:smart": "chmod +x fix-rollup-deps.sh && ./fix-rollup-deps.sh"
  },
  "dependencies": {
    "@fingerprintjs/fingerprintjs": "^5.0.1",
    "@heroicons/react": "^2.2.0",
    "@microsoft/clarity": "^1.0.2",
    "@prisma/client": "^7.4.1",
    "@radix-ui/react-dialog": "^1.1.15",
    "@react-email/components": "1.0.8",
    "@simplewebauthn/browser": "^13.2.2",
    "@simplewebauthn/server": "^13.2.3",
    "@simplewebauthn/types": "^12.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/jsdom": "^28.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/validator": "^13.15.10",
    "axios": "^1.13.5",
    "bcrypt": "^6.0.0",
    "buffer": "^6.0.3",
    "canvg": "^4.0.3",
    "chart.js": "^4.5.1",
    "clsx": "^2.1.1",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "docx": "^9.5.3",
    "dompurify": "^3.3.0",
    "express-rate-limit": "^7.5.0",
    "form-data": "^4.0.5",
    "framer-motion": "^12.34.3",
    "happy-tts-frontend": "file:",
    "html2canvas": "^1.4.1",
    "idb": "^8.0.3",
    "javascript-obfuscator": "^5.3.0",
    "jsonwebtoken": "^9.0.3",
    "jspdf": "^4.2.0",
    "jszip": "^3.10.1",
    "katex": "^0.16.28",
    "lodash.merge": "^4.6.2",
    "lucide-react": "^0.575.0",
    "marked": "^17.0.3",
    "marked-katex-extension": "^5.1.7",
    "md5": "^2.3.0",
    "mermaid": "^11.12.3",
    "prismjs": "^1.30.0",
    "process": "^0.11.10",
    "qrcode.react": "^4.2.0",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.3.1",
    "react-copy-to-clipboard": "^5.1.0",
    "react-debounce-input": "^3.3.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-inspector": "^9.0.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.13.1",
    "react-syntax-highlighter": "^16.1.0",
    "react-toastify": "^11.0.5",
    "rollup": "^4.59.0",
    "swagger-ui-react": "^5.31.1",
    "tailwind-merge": "^3.5.0",
    "util": "^0.12.5",
    "validator": "^13.15.23",
    "vite-plugin-auto-i18n": "^0.0.9"
  },
  "devDependencies": {
    "@rsbuild/core": "^1.7.3",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/javascript-obfuscator": "^0.17.0",
    "@types/jest": "^30.0.0",
    "@types/marked": "^6.0.0",
    "@types/md5": "^2.3.6",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.6",
    "@types/testing-library__jest-dom": "^6.0.0",
    "@vitejs/plugin-react": "^5.1.4",
    "@vitest/ui": "^4.0.6",
    "autoprefixer": "^10.4.24",
    "cross-env": "^10.1.0",
    "javascript-obfuscator": "^5.3.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^28.1.0",
    "postcss": "^8.5.6",
    "react-email": "5.2.8",
    "rollup-plugin-visualizer": "^7.0.0",
    "tailwind-scrollbar": "^3.1.0",
    "tailwindcss": "^3.4.17",
    "terser": "^5.46.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-auto-i18n-plugin": "^1.1.14",
    "vite-plugin-obfuscator": "^1.0.5",
    "vitest": "^4.0.6"
  },
  "pnpm": {
    "overrides": {
      "marked": "^16.0.0",
      "react": "^19.0.0",
      "react-dom": "^19.0.0",
      "lodash-es": ">=4.17.23",
      "minimatch": ">=10.2.1"
    },
    "peerDependencyRules": {
      "allowedVersions": {
        "react": "19",
        "react-dom": "19",
        "marked": "17"
      }
    }
  }
}
